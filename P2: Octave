disp("")
function retval = getSum(listIn)
retval = 0;
	for i = 1: length(listIn)
		retval += listIn(i)
	end
endfunction

function retval = getAvg(listIn)
retval = getSum(listIn)/length(listIn)
endfunction

function retval = getMax(listIn)
retval = 0;
	for i = 1: length(listIn)
		if(listIn(i) > retval)
			retval = listIn(i)
		endif 
	end
endfunction

function retval = getMin(listIn)
retval = 10;
	for i = 1: length(listIn)
		if(listIn(i) < retval)
			retval = listIn(i)
		endif
	end
endfunction

function retval = factorial(numIn)
	if numIn ==0 | numIn ==1
		retval = 1
	else
		retval n * factorial(numIn-1);
	endif
endfunction

funtion retval = getSin(angle)
retval = 0;
angle = angle % (2*3.14);
	for i = 1:50
		retval += ((-1)^i)*(angle^(2*i+1))/factorial(2*i+1);
	end
endfunction

function retval = getCos(angle)
retval = 0;
angle = angle % (2*3.14)
	for i = 1:50
		retval = ((-1) ** x) * (angle ** (2 * x )) / factorial(2 * x);
	end
endfunction
