function retval = getFreqTable(wordsIn, freqTable)
	for i = 1:length(wordsIn)
		found = false;
		for j = 1:length(freqTable)
			if strcmp(wordsIn{i}, freqTable{j}{1})
				freqTable{j}{2} = freqTable{j}{2} +1;
				found = true;
				break;
			endif
		end
		if found = false
			freqTable{end+1} = {wordsIn{i}, 1};
		endif
	end
	retval = freqTable
endfunction
			
	

